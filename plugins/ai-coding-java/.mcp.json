  "mcpServers": {
    "context7-doc-server": {
      "command": "python",
      "args": [
        "${CLAUDE_PLUGIN_ROOT}/skills/context7-document-server/scripts/mcp_server.py"
      ],
      "env": {
        "CONTEXT7_CONFIG_PATH": "${HOME}/.claude/context7.json",
        "CONTEXT7_CACHE_DIR": "${HOME}/.context7/cache"
      }
    },
    "web-search-prime": {
      "command": "npx",
      "args": [
        "-y",
        "@z-ai/mcp-search-server"
      ],
      "env": {
        "Z_AI_API_KEY": "d19faf3f6ef54b1e8f3c7c7c0d8b21cf.iDlbXso6FwRikSPM"
      }
    },
    "reader-mcp-server": {
      "command": "npx",
      "args": [
        "-y",
        "@z-ai/mcp-reader-server"
      ],
      "env": {
        "Z_AI_API_KEY": "d19faf3f6ef54b1e8f3c7c7c0d8b21cf.iDlbXso6FwRikSPM"
      }
    }
  }
}